{
  "name": "ai-music-player",
  "version": "1.0.0",
  "description": "AI Music Player web application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/init-db.js",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "audit:force-fix": "npm audit fix --force",
    "security:scan": "npm audit && npm run security:check-deps",
    "security:check-deps": "npm outdated || true",
    "security:report": "npm audit --json > security-audit.json && echo 'Security audit saved to security-audit.json'"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "music",
    "ai",
    "web-app"
  ],
  "author": "",
  "license": "MIT"
}
